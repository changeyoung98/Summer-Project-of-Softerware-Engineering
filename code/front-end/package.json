{
  "name": "final",
  "version": "1.0.0",
  "description": "a project for the webp frontend with webpack and react and antd",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --inline --content-base.",
    "build": "webpack",
    "dev": "NODE_ENV=development webpack-dev-server  --inline --progress --colors --port 3000 --host 0.0.0.0 ",
    "test": "jest",
    "coverage": "jest --colors --coverage"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      ".*\\.(css|less|scss)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "author": "yuxinru",
  "license": "ISC",
  "dependencies": {
    "mochawesome": "^3.0.2",
    "react": "^0.14.7",
    "react-cropper": "^1.0.1",
    "react-dom": "^0.14.7",
    "react-dropzone": "^4.2.13",
    "react-router": "^2.0.0"
  },
  "devDependencies": {
    "antd": "^2.13.14",
    "babel-core": "^6.5.1",
    "babel-jest": "^23.4.0",
    "babel-loader": "^6.2.2",
    "babel-plugin-import": "^1.7.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-14": "^1.0.6",
    "http-server": "^0.11.1",
    "jest": "^23.4.1",
    "react-addons-test-utils": "^0.14.7",
    "style-loader": "^0.21.0",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
  }
}
